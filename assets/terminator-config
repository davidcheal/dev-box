  smart_copy = False
[keybindings]
  paste = <Primary>v
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    scrollback_infinite = True
    copy_on_selection = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
